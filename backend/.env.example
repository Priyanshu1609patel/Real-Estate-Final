# Map MYSQL_ variables to the DB_ variables your code expects
DB_HOST=${MYSQLHOST}
DB_PORT=${MYSQLPORT}
DB_USER=${MYSQLUSER}
DB_PASSWORD=${MYSQLPASSWORD}
DB_NAME=${MYSQLDATABASE}

# Other configs
PORT=3001
NODE_ENV=production
MYSQL_SSL=true

# JWT Configuration
JWT_SECRET=your_secure_random_string_here
JWT_EXPIRES_IN=30d

# CORS Configuration
CORS_ORIGIN=*
